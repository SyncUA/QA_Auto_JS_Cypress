{
    "name": "qa_auto_js_cypress",
    "version": "1.0.0",
    "description": "",
    "homepage": "https://github.com/SyncUA/QA_Auto_JS_Cypress#readme",
    "bugs": {
        "url": "https://github.com/SyncUA/QA_Auto_JS_Cypress/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SyncUA/QA_Auto_JS_Cypress.git"
    },
    "license": "ISC",
    "author": "",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint": "eslint .",
        "lint-fix": "eslint . --fix",
        "format": "prettier .",
        "format-fix": "prettier . --write",
        "cypress-open": "npx cypress open",
        "cypress-open-dev": "npx cypress open --env configFile=dev",
        "cypress-open-prod": "npx cypress open --env configFile=prod",
        "cypress-run": "npx cypress run",
        "cypress-run-dev": "npx cypress run --env configFile=dev",
        "cypress-run-prod": "npx cypress run --env configFile=prod",
        "cypress-run-dev-mochawesome": "npx cypress run --env configFile=dev --env reporter=mochawesome",
        "cypress-run-prod-mochawesome": "npx cypress run --env configFile=prod --env reporter=mochawesome",
        "manual-mochawesome-merge-json": "npx mochawesome-merge cypress/reports/mochawesome/mochawesome*.json > cypress/reports/mochawesome/combined.json",
        "manual-mochawesome-generate-html": "npx marge cypress/reports/mochawesome/combined.json --reportDir cypress/reports/mochawesome --inline",
        "generate-mochawesome-report": "node cypress/support/generate-mochawesome-report.js",
        "cy.dev.mochawesome": "npm run cypress-run-dev-mochawesome && npm run generate-mochawesome-report",
        "cy.prod.mochawesome": "npm run cypress-run-prod-mochawesome && npm run generate-mochawesome-report",
        "cypress-run-dev-allure": "npx cypress run --env configFile=dev --env reporter=allure",
        "cypress-run-prod-allure": "npx cypress run --env configFile=prod --env reporter=allure",
        "manual-allure-generate": "allure generate cypress/reports/allure-results --clean -o cypress/reports/allure-report",
        "manual-allure-open": "allure open cypress/reports/allure-report",
        "generate-allure-report": "node cypress/support/generate-allure-report.js",
        "cy.dev.allure": "npm run cypress-run-dev-allure && npm run generate-allure-report",
        "cy.prod.allure": "npm run cypress-run-prod-allure && npm run generate-allure-report"
    },
    "devDependencies": {
        "@eslint/js": "^9.37.0",
        "allure-commandline": "^2.34.1",
        "allure-cypress": "^3.4.1",
        "cypress": "^15.5.0",
        "eslint": "^9.37.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "globals": "^16.4.0",
        "mocha-multi-reporters": "^1.5.1",
        "mochawesome": "^7.1.4",
        "mochawesome-merge": "^5.0.0",
        "mochawesome-report-generator": "^6.3.0",
        "prettier": "^3.6.2",
        "typescript-eslint": "^8.45.0"
    }
}
